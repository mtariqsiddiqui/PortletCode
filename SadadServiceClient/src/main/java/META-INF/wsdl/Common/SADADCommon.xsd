<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.sadad.com/scm/Common/1.0" version="1.0" xmlns="http://www.sadad.com/scm/Common/1.0" xmlns:err="http://www.sadad.com/scm/Error/1.0" xmlns:sys="http://www.sadad.com/scm/Common/System/1.0" xmlns:tns="http://www.sadad.com/scm/Common/1.0">

	<xs:import namespace="http://www.sadad.com/scm/Error/1.0" schemaLocation="Error.xsd"/>
	<xs:import namespace="http://www.sadad.com/scm/Common/System/1.0" schemaLocation="SADADSystemTypes.xsd"/>

	<xs:complexType abstract="true" name="BaseType">
		<xs:annotation>
			<xs:documentation>This is the base type from which all request and response elements are extended.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="MessageHeader"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="MessageHeader" type="MessageHeader_Type"/>
	<xs:complexType name="MessageHeader_Type">
		<xs:annotation>
			<xs:documentation>MessageHeader to be included in all Service request and response </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ServiceInitiatorKey"/>
			<xs:element ref="ServiceProviderKey"/>
			<xs:element ref="ServiceConsumerId"/>
			<xs:element minOccurs="0" ref="RqUID"/>
			<xs:element ref="UUID"/>
			<xs:element ref="Date"/>
			<xs:element ref="Lang"/>
			<xs:element minOccurs="0" ref="EndUserID"/>
			<xs:element minOccurs="0" ref="ApplicationID"/>
			<xs:element minOccurs="0" ref="Compressed"/>
			<xs:element minOccurs="0" ref="MsgCode"/>
			<xs:element minOccurs="0" ref="ServiceMsgType"/>
			<xs:element minOccurs="0" ref="Channel"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="LookupData_Type">
		<xs:annotation>
			<xs:documentation>Lookup data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LookupKey" type="sys:C32_SType"/>
			<xs:element name="Status" type="ConfigurationStatus_Enum"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Property" type="Property_Type"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="Property_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="sys:C32_SType"/>
			<xs:element name="Value" type="sys:C64_SType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="PartnerInfo" type="PartnerInfo_Type"/>
	<xs:complexType name="PartnerInfo_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Partner in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PartnerKey"/>
			<xs:element ref="Name"/>
			<xs:element minOccurs="0" name="Type" type="PartnerType_Enums"/>
			<xs:element minOccurs="0" name="Status" type="xs:string"/>
			<xs:element minOccurs="0" name="Category" type="PartnerCategory_SType"/>
			<xs:element default="false" minOccurs="0" name="FeeServiceProvider" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Partner_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Partner in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PartnerInfo_Type">
					<xs:sequence>
						<xs:element minOccurs="0" ref="Description"/>
						<xs:choice minOccurs="0">
							<xs:element ref="BankFinanicialInfo"/>
							<xs:element ref="BillerFinanicialInfo"/>
							<xs:element ref="SADADFinanicialInfo"/>
						</xs:choice>
						<xs:element minOccurs="0" ref="CreateTime"/>
						<xs:element minOccurs="0" ref="LastUpdateTime"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BankFinanicialInfo" type="BankFinanicialInfo_Type"/>
	<xs:element name="BillerFinanicialInfo" type="BillerFinanicialInfo_Type"/>
	<xs:element name="SADADFinanicialInfo" type="SADADFinanicialInfo_Type"/>
	<xs:complexType name="PartnerDetails_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Partner in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Partner_Type">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="Configurations" type="PartnerConfiguration_Type"/>
					<xs:element minOccurs="0" name="ServiceSubscription" type="PartnerSubscriptions_Type"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SADADFinanicialInfo_Type">
		<xs:sequence>
			<xs:element name="SADADCurrentAccount" type="FinancialAccount_Type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BankFinanicialInfo_Type">
		<xs:sequence>
			<xs:element minOccurs="0" name="SarieKey" type="sys:C64_SType"/>
			<xs:element name="SADADSuspenseAccount" type="sys:C64_SType"/>
			<xs:element name="SADADRefundAccount" type="sys:C64_SType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BillerFinanicialInfo_Type">
		<xs:sequence>
			<xs:element name="BillingAccount" type="FinancialAccount_Type"/>
			<xs:element name="RefundAccount" type="FinancialAccount_Type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="FinancialAccount_Type">
		<xs:sequence>
			<xs:element ref="BankKey"/>
			<xs:element minOccurs="0" ref="BankName"/>
			<xs:element ref="BankAccount"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="RuleType_Enum">
		<xs:annotation>
			<xs:documentation>Defines the closed enum for Lookup types</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CIP"/>
			<xs:enumeration value="DPV"/>
			<xs:enumeration value="ACCOUNT_OWNER_ID_VALIDATION"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  Service Registery Event Struture -->

	  <xs:element name="ServiceRegistryMsg" type="ServiceRegistryMsg"/>

  <xs:complexType name="Target">
    <xs:sequence>
      <xs:element minOccurs="0" name="name" type="xs:string"/>
      <xs:element minOccurs="0" name="bsrURI" type="xs:string"/>
      <xs:element minOccurs="0" name="description" type="xs:string"/>
      <xs:element minOccurs="0" name="creationTimestamp" type="xs:string"/>
      <xs:element minOccurs="0" name="LastModified" type="xs:string"/>
      <xs:element minOccurs="0" name="LastModifiedBy" type="xs:string"/>
      <xs:element minOccurs="0" name="Namespace" type="xs:string"/>
      <xs:element minOccurs="0" name="Owner" type="xs:string"/>
      <xs:element minOccurs="0" name="Version" type="xs:string"/>
      <xs:element name="userProperties">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                  <xs:element minOccurs="0" name="value" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Relation">
    <xs:sequence>
      <xs:element minOccurs="0" name="name" type="xs:string"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="targets" nillable="true" type="Target"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SRObject">
    <xs:sequence>
      <xs:element minOccurs="0" name="bsruri" type="xs:string"/>
      <xs:element minOccurs="0" name="type" type="xs:string"/>
      <xs:element minOccurs="0" name="name" type="xs:string"/>
      <xs:element minOccurs="0" name="description" type="xs:string"/>
      <xs:element minOccurs="0" name="namespace" type="xs:string"/>
      <xs:element minOccurs="0" name="version" type="xs:string"/>
      <xs:element minOccurs="0" name="transition" type="xs:string"/>
      <xs:element minOccurs="0" name="classificationURIs" type="xs:string"/>
      <xs:element name="userProperties">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                  <xs:element minOccurs="0" name="value" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="userRelationships" nillable="true" type="Relation"/>
      <xs:element minOccurs="0" name="namedQuery" type="NQResult"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="NQResult">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="result" nillable="true" type="ResultEntry"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ResultEntry">
    <xs:sequence>
      <xs:element name="resultEntries">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                  <xs:element minOccurs="0" name="value" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ServiceRegistryMsg">
    <xs:sequence>
      <xs:element minOccurs="0" name="event" type="xs:string"/>
      <xs:element minOccurs="0" name="oldSRObject" type="SRObject"/>
      <xs:element minOccurs="0" name="newSRObject" type="SRObject"/>
    </xs:sequence>
  </xs:complexType>

	<!--  Enterprise Data Event Structure -->
	<xs:element name="EventKey" type="xs:string"/>
	<xs:complexType name="EventAttributes_Type">
		<xs:annotation>
			<xs:documentation>
				This will be populated only if the Event is for Partner Related Data
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="PartnerKey"/>
			<xs:choice>
				<xs:element minOccurs="0" ref="ConfigurationName">
					<xs:annotation>
						<xs:documentation>
							This will be exist only of the Event is related to a configuration Change
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element minOccurs="0" ref="ServiceID">
						<xs:annotation>
							<xs:documentation>
								This will be exist only of the Event is related to a Service Subscription
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="Version">
						<xs:annotation>
							<xs:documentation>
								This will exist only of the Event is related to a Service subscription Change
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="MinorVersion">
						<xs:annotation>
							<xs:documentation>
								This will exist only of the Event is related to a configuration Change
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EnterpriseDataEvent_Type">
		<xs:sequence>
			<xs:element ref="EventKey">
				<xs:annotation>
					<xs:documentation>
						This will be populated according to the Event Type as follows:
						AccessChannelList: System Access Channels List Event
						DisctrictCodeList: System District Codes List Event
						PaymentMethodList: System Payment Methods List Event
						BillerServiceCodes: Fee Service Provider Service Codes Event
						PartnerIdentity: Partner Identity Event
						ParnerSubscription: Partner Service Subscription Event
						BankBarachCodes: Bank Branch Codes Even

						Occurnces:
						Create, Update Status
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="EventAttributes" type="EventAttributes_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="EnterpriseDataEvent" type="EnterpriseDataEvent_Type"/>

	<xs:simpleType name="ConfigurationStatus_Enum">
		<xs:annotation>
			<xs:documentation>Defines the known values for Rule Status</xs:documentation>
		</xs:annotation>
		<xs:restriction base="sys:NC32_SType">
			<xs:enumeration value="ACTIVE"/>
			<xs:enumeration value="INACTIVE"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="PartnerConfigurations" type="PartnerConfigurations_Type"/>
	<xs:complexType name="PartnerConfigurations_Type">
		<xs:sequence>
			<xs:element name="Configuration" type="Configuration_Type"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="PartnerConfiguration" type="PartnerConfiguration_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="PartnerConfiguration" type="PartnerConfiguration_Type"/>

	<xs:complexType name="PartnerConfigurationAttributes_Type">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="Attribute" type="PartnerConfigurationAttribute_Type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PartnerConfigurationTemplate_Type">
		<xs:sequence>
			<xs:element minOccurs="0" name="Id" type="xs:long"/>
			<xs:element name="Identifier" type="sys:NC20_SType"/>
			<xs:element name="TemplateId" type="xs:long"/>
			<xs:element minOccurs="0" ref="TemplateName"/>
			<xs:element minOccurs="0" name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartnerConfiguration_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="ConfigurationId" type="xs:long"/>
			<xs:element ref="ConfigurationName"/>
			<xs:element minOccurs="0" name="Template" type="PartnerConfigurationTemplate_Type"/>
			<xs:element minOccurs="0" name="Attributes" type="PartnerConfigurationAttributes_Type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="Configuration" type="Configuration_Type"/>
	<xs:complexType name="Configuration_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Id" type="xs:long"/>
			<xs:element name="Name" type="sys:NC100_SType"/>
			<xs:element name="Description" type="sys:NC100_SType"/>
			<xs:element maxOccurs="unbounded" name="Attributes" type="ConfigurationAttribute_Type"/>
			<xs:element minOccurs="0" name="TemplatesData" type="ConfigurationTemplateData_Type"/>
			<xs:element minOccurs="0" name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ConfigurationTemplateData_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Configuration Templates</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="IdentifierName"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Identifiers" type="sys:NC20_SType"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Templates" type="ConfigurationTemplate_Type"/>

		</xs:sequence>
	</xs:complexType>

	<xs:element name="ConfigurationTemplate" type="ConfigurationTemplate_Type"/>
	<xs:complexType name="ConfigurationTemplate_Type">
		<xs:annotation>
			<xs:documentation>Configuration Template Deifinition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="Id" type="xs:long"/>
			<xs:element ref="TemplateName"/>
			<xs:element name="Description" type="sys:C256_SType"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="Attributes" type="TemplateAttribute_Type"/>
			<xs:element minOccurs="0" name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConfigurationAttribute_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup/entity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="Id" type="xs:long"/>
			<xs:element name="Name" type="sys:NC100_SType"/>
			<xs:element name="Description" type="sys:C256_SType"/>
			<xs:element name="DefaultValue" type="sys:C256_SType"/>
			<xs:element default="false" name="IsTemplateIdentifier" type="xs:boolean"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="PossibleValue" type="sys:C256_SType"/>
			<xs:element minOccurs="0" name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TemplateAttribute_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup/entity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="Id" type="xs:long"/>
			<xs:element name="AttributeId" type="xs:long"/>
			<xs:element minOccurs="0" name="AttributeName" type="sys:NC100_SType"/>
			<xs:element name="Value" type="sys:C256_SType"/>
			<xs:element name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartnerConfigurationAttribute_Type">
		<xs:annotation>
			<xs:documentation>To hold additional properties for a Lookup/entity</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="Id" type="xs:long"/>
			<xs:element maxOccurs="unbounded" name="Value" type="sys:C256_SType"/>
			<xs:element name="AttributeId" type="xs:long"/>
			<xs:element minOccurs="0" name="AttributeName" type="sys:NC100_SType"/>
			<xs:element minOccurs="0" name="Status" type="ConfigurationStatus_Enum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Fault_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Partner in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="FaultCode"/>
			<xs:element ref="FaultString"/>
			<xs:element ref="FaultActor"/>
		</xs:sequence>
	</xs:complexType>



	<xs:element name="CreateTime" type="sys:Date_SType"/>
	<xs:element name="LastUpdateTime" type="sys:Date_SType"/>
	<xs:element name="Fault_Type" type="Fault_Type"/>
	<xs:element name="FaultCode" type="sys:NC16_SType"/>
	<xs:element name="FaultString" type="sys:C128_SType"/>
	<xs:element name="FaultActor" type="xs:long"/>
	<xs:complexType name="Status_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Status in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="StatusCode"/>
			<xs:element minOccurs="0" ref="StatusDesc"/>
			<xs:element ref="err:Severity"/>
			<xs:element minOccurs="0" ref="err:Parameters"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Status" type="Status_Type"/>
	<xs:element name="StartDate" type="sys:Date_SType"/>
	<xs:element name="EndDate" type="sys:Date_SType"/>
	<xs:element name="ProcessDate" type="sys:Date_SType"/>
	<xs:element name="TransactionDate" type="sys:Date_SType"/>
	<xs:element name="AccountSource" type="AccountSource_Enum"/>
	<xs:complexType name="AccountId_Type">
		<xs:annotation>
			<xs:documentation>-Represents an Account in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" ref="AccountKey"/>
			<xs:element ref="BillerKey"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AccountInfo_Type">
        <xs:sequence>
			<xs:element ref="AccountKey"/>
			<xs:element ref="BillerKey"/>
			<!-- added for EBPP Query end-->
			<xs:element minOccurs="0" name="BillerName" type="sys:Identifier_SType"/>
			<xs:element ref="ServiceType"/>
			<xs:element minOccurs="0" ref="Lifecycle"/>
			<xs:element minOccurs="0" ref="AccountSource"/>
			<xs:element minOccurs="0" ref="Beneficiary"/>
			<xs:element minOccurs="0" ref="CreateDate"/>
	    </xs:sequence>
	</xs:complexType>

	<xs:element name="Lifecycle" type="AccountLifecycle_Enums"/>

	<xs:element name="BeneficiaryPhoneNum" type="sys:Phone_SType"/>

	<xs:complexType name="Bill_Type">
		<xs:annotation>
			<xs:documentation>-Represents a Bill in SADAD systems   </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" ref="BillKey"/>
			<xs:element minOccurs="0" ref="AccountKey"/>
			<xs:element ref="BillerKey"/>
			<xs:element ref="BillCategory"/>
			<xs:element ref="ServiceType"/>
			<xs:element ref="AmountDue"/>
			<xs:element minOccurs="0" ref="DueDate"/>
			<xs:element minOccurs="0" ref="BillCycle"/>
			<xs:element minOccurs="0" ref="GenerationDate"/>
			<xs:element minOccurs="0" ref="OpenDate"/>
			<xs:element minOccurs="0" ref="CloseDate"/>
			<xs:element minOccurs="0" ref="ExpiryDate"/>
			<xs:element minOccurs="0" ref="MinPartialAmt"/>
			<xs:element minOccurs="0" ref="MaxAdvanceAmt"/>
			<xs:element minOccurs="0" ref="MinAdvanceAmt"/>
			<xs:element minOccurs="0" ref="IsNewCustomer"/>
			<xs:element minOccurs="0" ref="CheckDigit"/>
			<xs:element minOccurs="0" ref="BillRefInfo"/>
			<xs:element minOccurs="0" ref="BillLifecycle"/>
			<xs:element minOccurs="0" ref="BillType"/>
			<xs:element minOccurs="0" ref="BillStatusCode"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" ref="BillPmtStatus"/>
			<xs:element maxOccurs="unbounded" minOccurs="0" ref="BillSummAmt"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentInfo_Type">
		<xs:sequence>
			<xs:element ref="Payment"/>
			<xs:element ref="PaymentReference"/>
			<xs:element ref="PaymentExtn"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PaymentExtn_Type">
		<xs:annotation>
			<xs:documentation>-Payment Extension- Biller specific attributes go here </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" ref="ServiceType"/>
			<xs:element minOccurs="0" ref="PaymentTypeCode"/>
			<xs:element minOccurs="0" ref="Beneficiary"/>
			<xs:element default="false" minOccurs="0" name="PickedForRecon" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentReference_Type">
		<xs:sequence>
			<xs:element ref="BillerKey"/>
			<!-- added for EBPP Query end-->
			<xs:element minOccurs="0" name="BillerName" type="sys:Identifier_SType"/>
			<xs:choice>
				<xs:sequence>
					<xs:element minOccurs="0" ref="AccountKey"/>
					<xs:element minOccurs="0" ref="BillKey"/>
